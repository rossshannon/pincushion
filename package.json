{
  "name": "pincushion",
  "keywords": [
    "pinboard",
    "pinboard.in",
    "tagging",
    "bookmarks",
    "bookmarking",
    "delicious",
    "bookmarklet"
  ],
  "description": "Pincushion presents an enhanced version of the Pinboard.in 'Add Bookmark' page, which takes advantage of the capabilities of modern browsers",
  "version": "2.1.0",
  "author": "Ross Shannon <ross.shannon@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:rossshannon/pincushion.git"
  },
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "ladda": "^1.0.6",
    "openai": "^4.11.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.10.1",
    "react-transition-group": "^4.4.5",
    "react-window": "^1.8.11",
    "redux": "^4.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "babel-jest": "^29.7.0",
    "browserslist": "^4.22.1",
    "eslint": "^8.57.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.5.2",
    "npm-check": "^5.9.2",
    "prettier": "^3.0.3",
    "redux-mock-store": "^1.5.5",
    "redux-thunk": "^2.4.2",
    "trim-newlines": "^5.0.0",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "npm run test:ci",
    "test:ci": "jest --config jest.config.js",
    "test:watch": "jest --config jest.config.js --watch",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"**/*.test.{js,jsx,ts,tsx}\"",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "eject": "react-scripts eject"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --fix",
    "**/*.test.{js,jsx,ts,tsx}": "eslint --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!axios/).+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ]
  }
}
